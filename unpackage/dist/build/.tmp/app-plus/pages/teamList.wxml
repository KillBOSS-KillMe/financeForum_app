<view class="teamList"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="team"><view class="head"><block wx:if="{{index=='one'}}"><text class="title">一级代理</text></block><block wx:if="{{index=='two'}}"><text class="title">二级代理</text></block><view data-event-opts="{{[['tap',[['getMore',[index]]]]]}}" bindtap="__e">查看更多 >></view></view><view class="list"><block wx:for="{{item.data}}" wx:for-item="itemList" wx:for-index="indexList" wx:key="indexList"><block><view class="item" data-item="{{itemList}}" data-index="{{index}}" data-event-opts="{{[['tap',[['userDetail',['$event']]]]]}}" bindtap="__e"><image src="{{imgUrl+itemList.avatar}}" mode></image><text>{{itemList.name}}</text></view></block></block></view></view></block></block></view>