<view class="meTeamList"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="item"><view class="left"><block wx:if="{{item.avatar!=''}}"><image src="{{imgUrl+item.avatar}}" mode></image></block><block wx:else><image src="../static/user.png" mode></image></block><view class="info"><text class="title">{{item.name}}</text><view><block wx:if="{{item.type=='normal'}}"><text>{{item.deploy.userlevel.level_name}}</text></block><block wx:if="{{item.type=='member'}}"><text style="background-color:#C6A25D;">{{item.deploy.vipuserlevel.level_name}}</text></block><text hidden="{{!(item.invitees_level==0)}}">团队长</text></view><text class="time">{{"加入时间："+item.created_at}}</text></view></view><view data-event-opts="{{[['tap',[['getTel',['$0'],[[['list','',index,'mobile']]]]]]]}}" class="tel" bindtap="__e"><uni-icons class="iconfont icondianhua" vue-id="{{'1-'+index}}" type bind:__l="__l"></uni-icons></view></view></block></block><block wx:if="{{list.length==0}}"><view class="null">暂无数据</view></block></view>