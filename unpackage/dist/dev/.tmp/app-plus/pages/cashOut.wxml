<view class="cashOut"><view class="cash"><text class="cashMoney">提现金额</text><view class="cashInput"><label for>￥</label><input type="digit" value="" placeholder="请输入提现金额" data-event-opts="{{[['input',[['inputValue',['$event']]]]]}}" bindinput="__e"/></view><view class="displayCash"><view class="item">可提现金额<text>{{"￥"+collectionList.user_blance}}</text></view></view></view><view class="cash"><text class="cashMoney">选择提现账户</text><view><radio-group style="width:690rpx;display:flex;" id="{{index}}" data-event-opts="{{[['change',[['radioChange',['$event']]]]]}}" bindchange="__e"><view class="item"><label class="radio"><radio class="itemRadio" value="0"></radio><text>微信</text></label></view><view class="item"><label class="radio"><radio class="itemRadio" value="1"></radio><text>支付宝</text></label></view></radio-group><block wx:if="{{types==''}}"><image class="cashImg" src="../static/imgLost.png" mode></image></block><block wx:if="{{types=='0'}}"><view data-event-opts="{{[['tap',[['getCard',[1]]]]]}}" bindtap="__e"><block wx:if="{{imgShow==''}}"><image class="cashImg" src="../static/imgLost.png" mode></image></block><block wx:else><image class="cashImg" src="{{imgShow}}" mode></image></block></view></block><block wx:if="{{types=='1'}}"><view data-event-opts="{{[['tap',[['getCard',[2]]]]]}}" bindtap="__e"><block wx:if="{{imgAlipayShow==''}}"><image class="cashImg" src="../static/imgLost.png" mode></image></block><block wx:else><image class="cashImg" src="{{imgAlipayShow}}" mode></image></block></view></block><block wx:if="{{types=='1'}}"><text class="tip">请上传支付宝收款码</text></block><block wx:else><block wx:if="{{types=='0'}}"><text class="tip">请上传微信收款码</text></block><block wx:else><text class="tip">请选择提现方式</text></block></block></view><button class="cashButton" type data-event-opts="{{[['tap',[['cashOut',['$event']]]]]}}" bindtap="__e">预计3个工作日到账，确认提现</button></view></view>