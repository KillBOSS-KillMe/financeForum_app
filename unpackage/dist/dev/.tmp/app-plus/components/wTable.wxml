<view class="w_table"><view class="w_table_swper"><view class="w_table_content"><view class="w-table_title"><block wx:for="{{columns}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['w-table_title_item',(border)?'w-table_title_item_border':'']}}" style="{{'width:'+(item.width?item.width+'upx':'230upx')+';'+('text-align:'+(item.align||'left')+';')}}"><block wx:if="{{item.title}}"><text class="w_table_title_content">{{item.title}}</text></block></view></block></view><block wx:for="{{data}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{data.length}}"><view class="w_table_body"><block wx:for="{{columns}}" wx:for-item="col" wx:for-index="cIndex" wx:key="cIndex"><view data-event-opts="{{[['tap',[['clickRow',['$0',index],[[['data','',index]]]]]]]}}" class="w_table_body_col" style="{{'text-align:'+(col.align||'left')+';'}}" bindtap="__e"><view class="{{['col_cell',(border)?'col_cell_border':'']}}" style="{{'width:'+(col.width?col.width+'upx':'230upx')+';'}}"><view class="col_text"><block wx:if="{{col.type==='checkbox'}}"><checkbox checked="{{item._isChecked}}" data-event-opts="{{[['tap',[['checkItemCheckbox',['$0',index],[[['data','',index]]]]]]]}}" catchtap="__e"></checkbox></block><block wx:if="{{col.type==='index'}}"><view>{{''+(index+1)+''}}</view></block><block wx:if="{{col.render}}"><cell vue-id="{{'1-'+index+'-'+cIndex}}" row="{{item}}" column="{{col}}" index="{{index}}" render="{{col.render}}" bind:__l="__l"></cell></block><block wx:else><block wx:if="{{!col.render}}"><text><rich-text nodes="{{item[col.key]}}"></rich-text></text></block></block></view></view></view></block></view></block></block></view><block wx:if="{{data.length==0}}"><view class="w_table_null"><view>{{noDataText}}</view></view></block></view></view>