<view class="meTeamList"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="item" data-index="{{indexType}}" data-item="{{item}}" data-event-opts="{{[['tap',[['getDetail',['$event']]]]]}}" bindtap="__e"><view class="left"><block wx:if="{{item.avatar!=''}}"><image src="{{imgUrl+item.avatar}}" mode></image></block><block wx:else><image src="../static/user.png" mode></image></block><view class="info"><text class="title">{{item.name}}</text><text class="time">{{"加入时间："+item.created_at}}</text></view></view><view data-event-opts="{{[['tap',[['getTel',['$0'],[[['list','',index,'mobile']]]]]]]}}" class="tel" catchtap="__e"><uni-icons class="iconfont icondianhua" vue-id="{{'efcebf0c-1-'+index}}" type bind:__l="__l"></uni-icons></view></view></block></block><block wx:if="{{list.length==0}}"><view class="null">暂无数据</view></block></view>