<view class="index"><page-search vue-id="1" bind:__l="__l"></page-search><view class="page-section-spacing banner"><swiper class="swiper" indicator-dots="{{indicatorDots}}" indicator-color="rgba(255,255,255,.3)" indicator-active-color="#fff" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}"><block wx:for="{{bannerList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><swiper-item><image class="bannerImg" src="{{item.img}}" mode="aspectFill"></image></swiper-item></block></swiper></view><view class="nav"><block wx:for="{{nav}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block><view class="navList" data-id="{{item.id}}" data-event-opts="{{[['tap',[['goTo',['$event']]]]]}}" bindtap="__e"><image src="{{item.img}}" mode="aspectFill"></image><text>{{item.title}}</text></view></block></block></view><view class="content"><view class="inv-h-w"><view data-event-opts="{{[['tap',[['e0',['$event']]]]]}}" class="{{['inv-h',Inv==0?'inv-h-se':'']}}" bindtap="__e">最新产品解析</view><view data-event-opts="{{[['tap',[['e1',['$event']]]]]}}" class="{{['inv-h',Inv==1?'inv-h-se':'']}}" bindtap="__e">办卡提额技术</view></view><view class="contentList"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="item"><image src="{{item.img}}" mode="aspectFill"></image><view class="itemRight"><text class="title">{{item.title}}</text><view class="itemCon"><text>{{item.time}}</text><text>{{item.name}}</text><text>{{item.num+"评"}}</text></view></view></view></block><uni-load-more vue-id="2" loadingType="{{loadingType}}" contentText="{{contentText}}" bind:__l="__l"></uni-load-more></view></view></view>